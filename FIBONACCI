n1=0
n2=1
count=0
a= int( input("enter the no.of terms") )
if a<=1:
    print("The Fibonacciseries is: ",n1)
elif a==2:
    print("The Fibonacciseries is:",n1,n2)
else:
    print("The Fibonacciseries is:")
    while count< a:
        print(n1)
        y=n1+n2
        n1=n2
        n2=y
        count += 1
    
